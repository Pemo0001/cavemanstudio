---
import "../global.css";
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta name="format-detection" content="telephone=no" />
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="https://use.typekit.net/ipm2oth.css" />
    <title>{title}</title>
  </head>
  <body>
    <main>
      <div id="wrapper">
        <slot />
      </div>
    </main>
  </body>
</html>
